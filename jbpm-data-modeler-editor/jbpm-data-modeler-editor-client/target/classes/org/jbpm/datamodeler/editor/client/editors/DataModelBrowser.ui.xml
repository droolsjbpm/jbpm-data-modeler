<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
        >


    <gwt:VerticalPanel ui:field="mainPanel">

        <gwt:Label ui:field="modelName" ></gwt:Label>

        <b:CellTable ui:field="dataObjectsTable"   bordered="true" condensed="true" striped="true" styleName="table table-bordered table-striped table-hover"/>

        <gwt:HTML>Create new entity</gwt:HTML>

        <gwt:TextBox ui:field="newEntityName"></gwt:TextBox>
        <b:Button ui:field="newEntityButton">Create</b:Button>

    </gwt:VerticalPanel>

</ui:UiBinder>