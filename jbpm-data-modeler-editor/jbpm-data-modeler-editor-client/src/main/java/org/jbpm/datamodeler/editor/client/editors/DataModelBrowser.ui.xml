<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >

    <ui:style>

    </ui:style>

    <g:HTMLPanel>


        <gwt:VerticalPanel ui:field="mainPanel" styleName="row-fluid">
            <gwt:HTMLPanel>
                <h4>
                <gwt:Label ui:field="modelName"></gwt:Label>
                </h4>
                <gwt:ScrollPanel width="100%" height="390px">
                    <b:CellTable ui:field="dataObjectsTable" styleName="table table-condensed"/>
                </gwt:ScrollPanel>
            </gwt:HTMLPanel>
            <gwt:HTML>
                <h5>Create new entity</h5>
            </gwt:HTML>

            <gwt:TextBox ui:field="newEntityName"></gwt:TextBox>
            <b:Button ui:field="newEntityButton" styleName="btn btn-primary">Create</b:Button>

        </gwt:VerticalPanel>


    </g:HTMLPanel>

</ui:UiBinder>